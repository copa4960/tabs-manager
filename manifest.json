{
  "manifest_version": 3,
  "name": "Tab Manager for Chrome Dev Docs",
  "version": "1.0",
  "icons": {
    "16": "clipboard-pixel-art-vector.png",
    "32": "clipboard-pixel-art-vector.png",
    "48": "clipboard-pixel-art-vector.png",
    "128": "clipboard-pixel-art-vector.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://developer.chrome.com/*"],
  "permissions": ["tabGroups", "storage"]
}
